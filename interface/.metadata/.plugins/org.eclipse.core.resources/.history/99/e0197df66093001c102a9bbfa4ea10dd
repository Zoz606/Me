#include "typedef_keywords.h"
#ifndef LCD_H_
#define LCD_H_

#define CLR_SCREEN 0x01
#define CURSOR_ON_DISPLAY_ON 0x0e
#define RETURN_HOME 0x02
#define ENTRY_MODE 0x06
#define EN 0
#define RS 1
//Initialize the LCD
void enable(UINT8 port);
//Send a character to the data DDRAM
void LCD_vSendChar(UINT8 enablePort, UINT8 RsPort, UINT8 dataPort ,UINT8 data);
#endif /* LCD_H_ */
