#define F_CPU 8000000UL
#include <util/delay.h>
#include "BUTTON.h"
#include "LED.h"

int main(void) {
	BUTTON_vInit('A', 0);
	LED_vInit('D', 2);
	unsigned char returnVal;

	while(1) {
		returnVal = BUTTON_u8Read('A', 0);
		if (returnVal == 1 ) {
			LED_vToggle('D', 2);
			while(returnVal == 1) {

			}
		}
	}
}
